<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <!-- jar 包位置-->
    <!--<classPathEntry location="D:\Development tools\maven-repository\mysql\mysql-connector-java\5.1.46\mysql-connector-java-5.1.46.jar"/>-->
    <!--<classPathEntry location="D:\Development tools\maven-repository\com/microsoft\sqlserver\mssql-jdbc\6.4.0.jre8\mssql-jdbc-6.4.0.jre8.jar"/>-->
    <classPathEntry location="${jdbc.driverLocation}"/>
    <context id="context" targetRuntime="MyBatis3" defaultModelType="flat">
        <!-- 自动识别数据库关键字，默认false -->
        <property name="autoDelimitKeywords" value="true"/>

        <!-- 生成的Java文件的编码 -->
        <property name="javaFileEncoding" value="UTF-8"/>
        <!-- 格式化java代码 -->
        <property name="javaFormatter" value="org.mybatis.generator.api.dom.DefaultJavaFormatter"/>
        <!-- 格式化XML代码 -->
        <property name="xmlFormatter" value="org.mybatis.generator.api.dom.DefaultXmlFormatter"/>
        <!--可以使用``包括字段名，避免字段名与sql保留字冲突报错 -->
        <property name="beginningDelimiter" value=""/>
        <property name="endingDelimiter" value=""/>

        <!-- 增加爱Models Serializable实现 -->
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"/>

        <!-- include the plugin -->
        <plugin type="com.mybatis.page.plugin.LombokPlugin"/>
        <!--<plugin type="tk.mybatis.mapper.generator.MapperPlugin">
            <property name="mappers" value="com.scmofit.gifm.dao.TkBaseMapper"/>
        </plugin>-->
        <!-- 分页插件 -->
        <plugin type="com.mybatis.page.plugin.SelectByPagePlugin"></plugin>

        <!-- 此处是将Example改名为Criteria 当然 想改成什么都行 -->
        <plugin type="org.mybatis.generator.plugins.RenameExampleClassPlugin">
            <property name="searchString" value="Example$"/>
            <property name="replaceString" value="Criteria"/>
        </plugin>
        <!--<plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin"></plugin>-->

        <commentGenerator>
            <!--<property name="suppressAllComments" value="true" />-->
            <property name="suppressDate" value="true"/>
        </commentGenerator>

        <!-- jdbc连接信息 -->
        <jdbcConnection driverClass="${jdbc.driverClassName}"
                        connectionURL="${jdbc.url}"
                        userId="${jdbc.username}"
                        password="${jdbc.password}">
        </jdbcConnection>
        <!--<jdbcConnection driverClass="com.microsoft.sqlserver.jdbc.SQLServerDriver"-->
                        <!--connectionURL="jdbc:sqlserver://125.71.224.110:3673;databaseName=GIFM"-->
                        <!--userId="sa"-->
                        <!--password="SCsme202">-->
        <!--</jdbcConnection>-->
        <!-- 生成对象 -->
        <javaModelGenerator targetProject="src\main\java"
                            targetPackage="com.scmofit.gifm.pojo">
            <property name="enableSubPackages" value="true"/>
        </javaModelGenerator>
        <!-- 生成用于查询的Example对象 -->
        <sqlMapGenerator targetProject="src\main\resources"
                         targetPackage="mapperSecond">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <!-- 生成DAO的类文件以及配置文件 -->
        <javaClientGenerator targetProject="src\main\java"
                             targetPackage="com.scmofit.gifm.dao" type="XMLMAPPER">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

